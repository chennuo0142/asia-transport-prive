{% extends 'base.html.twig' %}

{% block title %}ContactMessage
{% endblock %}

{% block body %}
	<h1>ContactMessage</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ contact_message.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ contact_message.name }}</td>
			</tr>
			<tr>
				<th>Subject</th>
				<td>{{ contact_message.subject }}</td>
			</tr>
			<tr>
				<th>Email</th>
				<td>{{ contact_message.email }}</td>
			</tr>
			<tr>
				<th>Message</th>
				<td>{{ contact_message.message }}</td>
			</tr>
			<tr>
				<th>CreateAt</th>
				<td>{{ contact_message.createAt ? contact_message.createAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
			<tr>
				<th>ReadAt</th>
				<td>{{ contact_message.readAt ? contact_message.readAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
			<tr>
				<th>AnsweredAt</th>
				<td>{{ contact_message.answeredAt ? contact_message.answeredAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
			<tr>
				<th>TreatBy</th>
				<td>{{ contact_message.treatBy ? contact_message.treatBy|json_encode : '' }}</td>
			</tr>
			<tr>
				<th>Slug</th>
				<td>{{ contact_message.slug }}</td>
			</tr>
			<tr>
				<th>Phone</th>
				<td>{{ contact_message.phone }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_contact_message_index') }}">back to list</a>

	<a href="{{ path('app_contact_message_edit', {'slug': contact_message.slug}) }}">edit</a>

	{{ include('contact_message/_delete_form.html.twig') }}
{% endblock %}
